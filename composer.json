{
  "name": "makeitfly/grumphp-drupal",
  "type": "composer-plugin",
  "description": "GrumPHP + tools for Drupal",
  "require": {
    "composer-plugin-api": "^2.0",
    "phpro/grumphp": "^1.0",
    "squizlabs/php_codesniffer": "^3.4",
    "phpstan/phpstan": "^1.2",
    "drupal/coder": "^8.3",
    "mglaman/phpstan-drupal": "^1.1"
  },
  "extra": {
    "class": "Makeitfly\\GrumPhpDrupal\\Composer\\Plugins\\MakeitflyGrumPhpConfiguratorPlugin"
  },
  "autoload": {
    "psr-4": {
      "Makeitfly\\GrumPhpDrupal\\": "src/"
    }
  }
}
